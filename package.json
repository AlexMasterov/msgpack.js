  "peerDependencies": {
    "ascii-chr": "0.3.x",
    "utf8-bin": "0.3.x"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "fs-extra": "7.0.1",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "rollup": "0.67.3",
    "rollup-plugin-bundle-size": "1.0.2",
    "rollup-plugin-cjs-es": "0.7.0",
    "rollup-plugin-delete": "0.1.2",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-terser": "3.0.0"
  },
  "nyc": {
    "all": false,
    "cache": true,
    "sourceMap": true,
    "instrument": true,
    "reporter": [],
    "include": ["src/**/*.js"]
  }
