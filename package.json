{
  "name": "msgpuck",
  "version": "0.7.6",
  "description": "A fast and memory-efficient MessagePack serialization library",
  "keywords": [
    "messagepack",
    "msgpack",
    "binary",
    "serialization",
    "typescript"
  ],
  "license": "MIT",
  "author": "Alex Masterov <alex.masterow@gmail.com>",
  "repository": "AlexMasterov/msgpuck",
  "homepage": "https://github.com/AlexMasterov/msgpuck.js#readme",
  "bugs": {
    "url": "https://github.com/AlexMasterov/msgpuck.js/issues"
  },
  "main": "./index.js",
  "files": [
    "src/",
    "dist/",
    "./index.js"
  ],
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "test": "nyc mocha --timeout 0 test/*.test.js",
    "test-report": "nyc --reporter=html --reporter=text mocha test/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "ascii-chr": "0.3.0",
    "utf8-bin": "0.3.1"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "mocha": "5.2.0",
    "nyc": "13.1.0"
  },
  "nyc": {
    "all": false,
    "cache": true,
    "sourceMap": true,
    "instrument": true,
    "reporter": [],
    "include": [
      "src/**/*.js"
    ]
  }
}
